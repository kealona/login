<AnchorPane prefHeight="533.0" prefWidth="666.0" stylesheets="@CSS/ChatCSS.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.GroupChatController">
   <children>
      <Pane prefHeight="69.0" prefWidth="718.0">
         <children>
            <Label fx:id="group_Name" alignment="CENTER" layoutX="306.0" layoutY="16.0" prefHeight="38.0" prefWidth="106.0" text="Label">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <ListView fx:id="groupChatList" layoutY="69.0" prefHeight="325.0" prefWidth="518.0" />
      <ListView fx:id="groupFriendList" layoutX="518.0" layoutY="104.0" prefHeight="429.0" prefWidth="200.0" />
      <TextArea fx:id="Gmsg" layoutY="394.0" prefHeight="139.0" prefWidth="518.0" />
      <Label fx:id="GroupFriendsPage" layoutX="518.0" layoutY="69.0" prefHeight="36.0" prefWidth="200.0" text="群成员">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="sendBtn" layoutX="396.0" layoutY="479.0" mnemonicParsing="false" onAction="#clickSend" prefHeight="38.0" prefWidth="89.0" text="发送" />
