      <Button fx:id="file" layoutX="37.0" layoutY="358.0" mnemonicParsing="false" onAction="#clickFile" prefHeight="36.0" prefWidth="37.0" />
      <Button fx:id="image" layoutX="74.0" layoutY="358.0" mnemonicParsing="false" onAction="#clickPhoto" prefHeight="36.0" prefWidth="37.0" />
      <ImageView fitHeight="29.0" fitWidth="26.0" layoutX="678.0" layoutY="74.0" onMouseClicked="#clickAdd" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@CSS/image/邀请.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
